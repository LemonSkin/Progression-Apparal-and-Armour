<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <!-- Vneck_dress - v neck dress  -->
  <Operation Class="PatchOperationReplace">
    <xpath>Defs/ThingDef[defName="Vneck_dress"]/statBases</xpath>
    <value>
      <statBases>
        <MaxHitPoints>100</MaxHitPoints>
        <WorkToMake>2700</WorkToMake>
        <Mass>0.3</Mass>
        <EquipDelay>1.5</EquipDelay>
        <StuffEffectMultiplierArmor>0.1</StuffEffectMultiplierArmor>
        <StuffEffectMultiplierInsulation_Cold>0.10</StuffEffectMultiplierInsulation_Cold>
        <StuffEffectMultiplierInsulation_Heat>0.26</StuffEffectMultiplierInsulation_Heat>
      </statBases>
    </value>
  </Operation>
  <Operation Class="PatchOperationReplace">
    <xpath>Defs/ThingDef[defName="Vneck_dress"]/thingCategories</xpath>
    <value>
      <thingCategories Inherit="False">
        <li>ApparelNoble</li>
      </thingCategories>
    </value>
  </Operation>
