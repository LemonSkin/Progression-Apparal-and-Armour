<Patch>
  <!-- oracle_FireNationArmor -->
  <Operation Class="PatchOperationReplace">
    <xpath>Defs/ThingDef[defName="oracle_FireNationArmor"]/costStuffCount</xpath>
    <value>
      <costStuffCount>80</costStuffCount>
    </value>
  </Operation>
  <Operation Class="PatchOperationReplace">
    <xpath>Defs/ThingDef[defName="oracle_FireNationArmor"]/statBases</xpath>
    <value>
      <statBases>
        <MaxHitPoints>100</MaxHitPoints>
        <WorkToMake>3200</WorkToMake>
        <Mass>0.75</Mass>
        <StuffEffectMultiplierArmor>0.05</StuffEffectMultiplierArmor>
        <StuffEffectMultiplierInsulation_Cold>0.42</StuffEffectMultiplierInsulation_Cold>
        <StuffEffectMultiplierInsulation_Heat>0.3</StuffEffectMultiplierInsulation_Heat>
        <EquipDelay>3.5</EquipDelay>
      </statBases>
    </value>
  </Operation>
  <Operation Class="PatchOperationRemove">
    <xpath>Defs/ThingDef[defName="oracle_FireNationArmor"]/equippedStatOffsets</xpath>
  </Operation>
  <Operation Class="PatchOperationRemove">
    <xpath>Defs/ThingDef[defName="oracle_FireNationArmor"]/apparel/defaultOutfitTags</xpath>
  </Operation>
  <Operation Class="PatchOperationReplace">
    <xpath>Defs/ThingDef[defName="oracle_FireNationArmor"]/thingCategories</xpath>
    <value>
      <thingCategories Inherit="False">
        <li>LemonSkin_Costume</li>
      </thingCategories>
    </value>
  </Operation>


