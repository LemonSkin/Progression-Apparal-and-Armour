<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <!-- Apparel_UNA_weddingveil -->
  <Operation Class="PatchOperationReplace">
    <xpath>Defs/ThingDef[defName="Apparel_UNA_weddingveil"]/costStuffCount</xpath>
    <value>
      <costStuffCount>45</costStuffCount>
    </value>
  </Operation>
  <Operation Class="PatchOperationReplace">
    <xpath>Defs/ThingDef[defName="Apparel_UNA_weddingveil"]/statBases</xpath>
    <value>
      <statBases>
        <MaxHitPoints>80</MaxHitPoints>
        <WorkToMake>1800</WorkToMake>
        <Mass>0.07</Mass>
        <StuffEffectMultiplierArmor>0.0</StuffEffectMultiplierArmor>
        <StuffEffectMultiplierInsulation_Cold>0.0</StuffEffectMultiplierInsulation_Cold>
        <StuffEffectMultiplierInsulation_Heat>0.0</StuffEffectMultiplierInsulation_Heat>
        <EquipDelay>0.8</EquipDelay>
      </statBases>
    </value>
  </Operation>
  <Operation Class="PatchOperationRemove">
    <xpath>Defs/ThingDef[defName="Apparel_UNA_weddingveil"]/equippedStatOffsets</xpath>
  </Operation>
  <Operation Class="PatchOperationAdd">
    <xpath>Defs/ThingDef[defName="Apparel_UNA_weddingveil"]</xpath>
    <value>
      <thingCategories Inherit='False'>
        <li>LemonSkin_VanityHeadgear</li>
      </thingCategories>
    </value>
  </Operation>


