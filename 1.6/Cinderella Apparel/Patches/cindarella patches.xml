<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <!-- Apparel_CinderellaTiara -->
  <Operation Class="PatchOperationReplace">
    <xpath>Defs/ThingDef[defName="Apparel_CinderellaTiara"]/label</xpath>
    <value>
      <label>princess tiara</label>
    </value>
  </Operation>
  <Operation Class="PatchOperationReplace">
    <xpath>Defs/ThingDef[defName="Apparel_CinderellaTiara"]/costStuffCount</xpath>
    <value>
      <costStuffCount>45</costStuffCount>
    </value>
