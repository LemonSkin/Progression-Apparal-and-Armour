<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <!-- RBApparel_Tailcoat -->
  <Operation Class="PatchOperationReplace">
    <xpath>Defs/ThingDef[defName="RBApparel_Tailcoat"]/thingCategories</xpath>
    <value>
      <thingCategories Inherit="False">
        <li>LemonSkin_IndustrialApparel</li>
      </thingCategories>
    </value>
  </Operation>
  <Operation Class="PatchOperationReplace">
    <xpath>Defs/ThingDef[defName="RBApparel_Tailcoat"]/recipeMaker/researchPrerequisite</xpath>
    <value>
      <researchPrerequisites>
        <li>ComplexClothing</li>
        <li MayRequire="Ludeon.RimWorld.Royalty">NobleApparel</li>
      </researchPrerequisites>
    </value>
  </Operation>
</Patch>
