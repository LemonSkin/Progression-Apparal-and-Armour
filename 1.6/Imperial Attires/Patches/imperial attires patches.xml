<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <!-- CAT_Apparel_CrossBikiniTop -->
  <Operation Class="PatchOperationReplace">
    <xpath>Defs/ThingDef[defName="CAT_Apparel_CrossBikiniTop"]/label</xpath>
    <value>
      <label>cross bikini top</label>
    </value>
  </Operation>
  <Operation Class="PatchOperationReplace">
    <xpath>Defs/ThingDef[defName="CAT_Apparel_CrossBikiniTop"]/costStuffCount</xpath>
    <value>
      <costStuffCount>45</costStuffCount>
    </value>
  </Operation>
  <Operation Class="PatchOperationReplace">
    <xpath>Defs/ThingDef[defName="CAT_Apparel_CrossBikiniTop"]/statBases</xpath>
    <value>
      <statBases>
        <MaxHitPoints>100</MaxHitPoints>
        <WorkToMake>2700</WorkToMake>
        <Mass>0.3</Mass>
        <StuffEffectMultiplierArmor>0.05</StuffEffectMultiplierArmor>
        <StuffEffectMultiplierInsulation_Cold>0.0</StuffEffectMultiplierInsulation_Cold>
        <StuffEffectMultiplierInsulation_Heat>0.0</StuffEffectMultiplierInsulation_Heat>
        <EquipDelay>1.5</EquipDelay>
      </statBases>
    </value>
  </Operation>
  <Operation Class="PatchOperationRemove">
    <xpath>Defs/ThingDef[defName="CAT_Apparel_CrossBikiniTop"]/equippedStatOffsets</xpath>
  </Operation>
  <Operation Class="PatchOperationReplace">
    <xpath>Defs/ThingDef[defName="CAT_Apparel_CrossBikiniTop"]/thingCategories</xpath>
    <value>
      <thingCategories Inherit='False'>
        <li>LemonSkin_VanityApparel</li>
      </thingCategories>
    </value>
  </Operation>
  <Operation Class="PatchOperationReplace">
    <xpath>Defs/ThingDef[defName="CAT_Apparel_CrossBikiniTop"]/recipeMaker</xpath>
    <value>
      <recipeMaker Inherit="False">
        <recipeUsers Inherit="False">
          <li>Ferny_VanityTable</li>
        </recipeUsers>
        <researchPrerequisite Inherit="False">Ferny_VanityTable</researchPrerequisite>
      </recipeMaker>
    </value>
  </Operation>


